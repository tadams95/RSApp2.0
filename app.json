              "host": "ragestate.com",
              "pathPrefix": "/shop"
            },
            {
              "scheme": "https",
              "host": "www.ragestate.com",
              "pathPrefix": "/events"
            },
            {
              "scheme": "https",
              "host": "www.ragestate.com",
              "pathPrefix": "/user"
            },
            {
              "scheme": "https",
              "host": "www.ragestate.com",
              "pathPrefix": "/post"
            },
            {
              "scheme": "https",
              "host": "www.ragestate.com",
              "pathPrefix": "/transfer"
            },
            {
              "scheme": "https",
              "host": "www.ragestate.com",
              "pathPrefix": "/claim-ticket"
            },
            {
              "scheme": "https",
              "host": "www.ragestate.com",
              "pathPrefix": "/shop"
            }
          ],
          "category": ["BROWSABLE", "DEFAULT"]
        }
      ]
    },
    "web": {
      "favicon": "./src/assets/RSLogo2025.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos so you can choose your profile picture."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.tyrelle.ragestate-beta",
          "enableGooglePay": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/RSLogo2025.png",
          "color": "#000000"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "RAGESTATE needs access to your camera for QR scanning and ticket transfers.",
          "microphonePermission": "Allow RAGESTATE mic access for Admin Functions",
          "recordAudioAndroid": true
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": "com.googleusercontent.apps.930832370585-tvmm4moku77su7v4rp5adp3ql520gi73"
        }
      ],
      "expo-router",
      "expo-localization",
      [
        "expo-build-properties",
        {
          "ios": {
            "useFrameworks": "static"
          }
        }
