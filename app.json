{
  "expo": {
    "name": "RAGESTATE",
    "slug": "ragestate-app",
    "version": "2.43.0",
    "orientation": "portrait",
    "icon": "./assets/RSLogo2025.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./assets/RSLogo2025.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tyrelle.ragestateapp",
      "buildNumber": "43"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/RSLogo2025.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": ["android.permission.RECORD_AUDIO"],
      "package": "com.tyrelle.ragestate",
      "versionCode": 13
    },
    "web": {
      "favicon": "./assets/RSLogo2025.png"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos so you can choose your profile picture."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.tyrelle.ragestate-beta",
          "enableGooglePay": false
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./assets/RSLogo2025.png",
          "color": "#000000"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "Allow RAGESTATE to access your camera for QR scanning.",
          "microphonePermission": "Allow RAGESTATE mic access for Admin Functions",
          "recordAudioAndroid": true
        }
      ]
    ],
    "extra": {
      "eas": {
        "projectId": "0b623ccd-8529-45cb-bc54-dd7265c22a26"
      }
    },
    "owner": "tyrelle",
    "scheme": "ragestate"
  }
}
