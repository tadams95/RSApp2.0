{
  "expo": {
    "name": "RageState",
    "slug": "ragestate-app",
    "version": "2.47.0",
    "orientation": "portrait",
    "icon": "./src/assets/RSLogo2025.png",
    "userInterfaceStyle": "light",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/RSLogo2025.png",
      "resizeMode": "contain",
      "backgroundColor": "#000000",
      "hideExponentText": true
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.tyrelle.ragestateapp",
      "buildNumber": "46",
      "infoPlist": {
        "ITSAppUsesNonExemptEncryption": false
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/RSLogo2025.png",
        "backgroundColor": "#ffffff"
      },
      "googleServicesFile": "./google-services.json",
      "permissions": [
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA",
        "android.permission.RECORD_AUDIO",
        "android.permission.CAMERA"
      ],
      "package": "com.tyrelle.ragestate",
      "versionCode": 18
    },
    "web": {
      "favicon": "./src/assets/RSLogo2025.png",
      "bundler": "metro"
    },
    "plugins": [
      [
        "expo-image-picker",
        {
          "photosPermission": "The app accesses your photos so you can choose your profile picture."
        }
      ],
      [
        "@stripe/stripe-react-native",
        {
          "merchantIdentifier": "merchant.com.tyrelle.ragestate-beta",
          "enableGooglePay": true
        }
      ],
      [
        "expo-notifications",
        {
          "icon": "./src/assets/RSLogo2025.png",
          "color": "#000000"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermission": "RAGESTATE needs access to your camera for QR scanning and ticket transfers.",
          "microphonePermission": "Allow RAGESTATE mic access for Admin Functions",
          "recordAudioAndroid": true
        }
      ],
      "expo-router",
      "expo-localization"
    ],
    "extra": {
      "eas": {
        "projectId": "0b623ccd-8529-45cb-bc54-dd7265c22a26"
      }
    },
    "owner": "tyrelle",
    "scheme": "ragestate",
    "runtimeVersion": {
      "policy": "appVersion"
    },
    "updates": {
      "url": "https://u.expo.dev/0b623ccd-8529-45cb-bc54-dd7265c22a26"
    }
  }
}
